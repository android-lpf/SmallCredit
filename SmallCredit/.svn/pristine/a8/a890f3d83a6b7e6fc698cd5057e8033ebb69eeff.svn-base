package com.geo.smallcredit.vo;

public class SecondSelector {

	private String companyname;
	private String score;
	private String state;
	private int img;
	private String tigong;
	private String grade;
	private String time;

	public SecondSelector(int img,String companyname, String score, String state,
			 String tigong, String grade, String time) {
		super();
		this.companyname = companyname;
		this.score = score;
		this.state = state;
		this.img = img;
		this.tigong = tigong;
		this.grade = grade;
		this.time = time;
	}

	public SecondSelector() {
		super();
	}

	public String getCompanyname() {
		return companyname;
	}

	public void setCompanyname(String companyname) {
		this.companyname = companyname;
	}

	public String getScore() {
		return score;
	}

	public void setScore(String score) {
		this.score = score;
	}

	public String getState() {
		return state;
	}

	public void setState(String state) {
		this.state = state;
	}

	public int getImg() {
		return img;
	}

	public void setImg(int img) {
		this.img = img;
	}

	public String getTigong() {
		return tigong;
	}

	public void setTigong(String tigong) {
		this.tigong = tigong;
	}

	public String getGrade() {
		return grade;
	}

	public void setGrade(String grade) {
		this.grade = grade;
	}

	public String getTime() {
		return time;
	}

	public void setTime(String time) {
		this.time = time;
	}

}
