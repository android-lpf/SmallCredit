<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/my_break_line"
    android:fadingEdge="none"
    android:orientation="vertical"
    android:scrollbars="none" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/background_up" >

       <Button
            android:id="@+id/my_main_backbtn"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="6dp"
            android:background="@drawable/btn_general_back"
            android:visibility="gone"
             />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:text="我的"
            android:textColor="@color/color_white"
            android:textSize="20sp" />

        <ImageButton
            android:id="@+id/my_messageimgbtn"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            android:background="@drawable/msg_selector"
             />
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/my_break_line" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/my_break_line"
            android:orientation="vertical" >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1" >

                <ImageView
                    android:id="@+id/my_main_line1"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="@drawable/my_break_line" />

                <RelativeLayout
                    android:id="@+id/my_main_loginregister_realtive"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/my_main_line1"
                    android:layout_weight="1"
                    android:background="@color/blue_down"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/imageView1"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:background="@drawable/my_loginimg"
                        android:paddingBottom="20dp"
                        android:paddingTop="20dp" />

                    <TextView
                        android:id="@+id/my_main_loginregister"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="30dp"
                        android:layout_toRightOf="@id/imageView1"
                        android:paddingBottom="30dp"
                        android:paddingTop="30dp"
                        android:text="登录/注册"
                        android:textColor="@color/color_white"
                        android:textSize="16sp" />
                    <ImageView
                        android:id="@+id/my_main_loginregister_front"
                        style="@style/my_set_front_imageview"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
                        android:layout_marginTop="15dp" />
                </RelativeLayout>

                <ImageView
                    android:id="@+id/my_main_line2"
                    android:layout_width="match_parent"
                    android:layout_height="6dp"
                    android:layout_below="@id/my_main_loginregister_realtive"
                    android:background="@drawable/my_break_line" />

                <RelativeLayout
                    android:id="@+id/my_main_tel_realtive"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/my_main_line2"
                    android:layout_weight="1"
                    android:background="@drawable/my_group_break_line"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/imageView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:background="@drawable/my_renzheng" />

                    <ImageView
                        android:id="@+id/my_main_approve_front"
                        style="@style/my_set_front_imageview"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
                        android:layout_marginTop="15dp" />

                    <TextView
                        android:id="@+id/my_main_approve"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="40dp"
                        android:layout_toRightOf="@id/imageView2"
                        android:paddingBottom="15dp"
                        android:paddingTop="15dp"
                        android:text="我的认证"
                        android:textColor="@color/color_white"
                        android:textSize="16sp" />
                </RelativeLayout>

                <ImageView
                    android:id="@+id/my_main_line3"
                    android:layout_width="match_parent"
                    android:layout_height="6dp"
                    android:layout_below="@id/my_main_tel_realtive"
                    android:background="@drawable/my_break_line" />

                <RelativeLayout
                    android:id="@+id/my_main_progress_selector_relative"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/my_main_line3"
                    android:layout_weight="1"
                    android:background="@drawable/my_group_break_line"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/imageView3"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:background="@drawable/my_progress" />

                    <TextView
                        android:id="@+id/my_main_progress_selector"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="40dp"
                        android:layout_toRightOf="@id/imageView3"
                        android:paddingBottom="15dp"
                        android:paddingTop="15dp"
                        android:text="进度查询"
                        android:textColor="@color/color_white"
                        android:textSize="16sp" />

                    <ImageView
                        android:id="@+id/my_main_security_front"
                        style="@style/my_set_front_imageview"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
                        android:layout_marginTop="15dp" />
                </RelativeLayout>

                <ImageView
                    android:id="@+id/my_main_line4"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_below="@id/my_main_progress_selector_relative"
                    android:background="@drawable/my_break_line" />

                <RelativeLayout
                    android:id="@+id/my_main_letter_relative"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/my_main_line4"
                    android:layout_weight="1"
                    android:background="@drawable/my_group_break_line"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/imageView4"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:background="@drawable/my_jilu" />

                    <ImageView
                        android:id="@+id/my_main_security_front"
                        style="@style/my_set_front_imageview"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
                        android:layout_marginTop="15dp" />

                    <TextView
                        android:id="@+id/my_main_letter"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="40dp"
                        android:layout_toRightOf="@id/imageView4"
                        android:paddingBottom="15dp"
                        android:paddingTop="15dp"
                        android:text="交易记录"
                        android:textColor="@color/color_white"
                        android:textSize="16sp" />
                </RelativeLayout>

                <ImageView
                    android:id="@+id/my_main_line5"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_below="@id/my_main_letter_relative"
                    android:background="@drawable/my_break_line" />

                <RelativeLayout
                    android:id="@+id/my_main_bankmanger_relative"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/my_main_line5"
                    android:layout_weight="1"
                    android:background="@drawable/my_group_break_line"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/imageView6"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:background="@drawable/my_bankmanger" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="40dp"
                        android:layout_toRightOf="@id/imageView6"
                        android:paddingBottom="15dp"
                        android:paddingTop="15dp"
                        android:text="银行卡管理"
                        android:textColor="@color/color_white"
                        android:textSize="16sp" />

                    <ImageView
                        android:id="@+id/my_main_feedback_front"
                        style="@style/my_set_front_imageview"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
                        android:layout_marginTop="15dp" />
                </RelativeLayout>

                <ImageView
                    android:id="@+id/my_main_line6"
                    android:layout_width="match_parent"
                    android:layout_height="6dp"
                    android:layout_below="@id/my_main_bankmanger_relative"
                    android:background="@drawable/my_break_line" />

                <RelativeLayout
                    android:id="@+id/my_main_feedback_relative"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/my_main_line6"
                    android:layout_weight="1"
                    android:background="@drawable/my_group_break_line"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/imageView5"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:background="@drawable/my_payment" />

                    <ImageView
                        android:id="@+id/my_main_feedback_front"
                        style="@style/my_set_front_imageview"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
                        android:layout_marginTop="15dp" />

                    <TextView
                        android:id="@+id/my_main_payment"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="40dp"
                        android:layout_toRightOf="@id/imageView5"
                        android:paddingBottom="15dp"
                        android:paddingTop="15dp"
                        android:text="无忧还款"
                        android:textColor="@color/color_white"
                        android:textSize="16sp" />
                </RelativeLayout>

                <ImageView
                    android:id="@+id/my_main_line7"
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:layout_below="@id/my_main_feedback_relative"
                    android:background="@drawable/my_break_line" />

                <RelativeLayout
                    android:id="@+id/my_main_mybill_relative"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/my_main_line7"
                    android:layout_weight="1"
                    android:background="@drawable/my_group_break_line"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:background="@drawable/my_bill" />

                    <ImageView
                        android:id="@+id/my_main_about_front"
                        style="@style/my_set_front_imageview"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
                        android:layout_marginTop="15dp" />

                    <TextView
                        android:id="@+id/my_main_mybill"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="40dp"
                        android:layout_toRightOf="@+id/imageView7"
                        android:paddingBottom="15dp"
                        android:paddingTop="15dp"
                        android:text="我的账单"
                        android:textColor="@color/color_white"
                        android:textSize="16sp" />
                </RelativeLayout>

                <ImageView
                    android:id="@+id/my_main_line8"
                    android:layout_width="match_parent"
                    android:layout_height="6dp"
                    android:layout_below="@id/my_main_mybill_relative"
                    android:background="@drawable/my_break_line" />

                <RelativeLayout
                    android:id="@+id/my_main_more_relative"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/my_main_line8"
                    android:layout_weight="1"
                    android:background="@drawable/my_group_break_line"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/imageView8"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="20dp"
                        android:background="@drawable/my_more" />

                    <ImageView
                        android:id="@+id/my_main_more_front"
                        style="@style/my_set_front_imageview"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="15dp"
                        android:layout_marginTop="15dp" />

                    <TextView
                        android:id="@+id/my_main_more"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_marginLeft="40dp"
                        android:layout_toRightOf="@id/imageView8"
                        android:paddingBottom="15dp"
                        android:paddingTop="15dp"
                        android:text="更多"
                        android:textColor="@color/color_white"
                        android:textSize="16sp" />
                </RelativeLayout>

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="6dp"
                    android:layout_below="@id/my_main_more_relative"
                    android:background="@drawable/my_break_line" />
            </RelativeLayout>
        </LinearLayout>
    </ScrollView>

</LinearLayout>