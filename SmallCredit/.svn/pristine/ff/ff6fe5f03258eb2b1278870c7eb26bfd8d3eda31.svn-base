<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/my_break_line"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/reality"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/background_up" >

        <Button
            android:id="@+id/mysecurepaymentdepositresult_backbtn"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="6dp"
            android:background="@drawable/btn_general_back"
            android:padding="10dp" />

        <TextView
            android:id="@+id/mysecurepaymentdepositresult_backtext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="29dip"
            android:textColor="@android:color/white"
            android:textSize="12sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:text="提现结果"
            android:textColor="@color/color_white"
            android:textSize="20sp" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="2dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/reality"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/caise_bar" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/imageView1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp"
        android:text="转出申请已提交，等待银行处理"
        android:textColor="@color/color_white"
        android:textSize="14sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/mysecurepaymentdepositresult_datatime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/textView1"
        android:layout_below="@id/textView1"
        android:layout_marginTop="10dp"
        android:text="2015-06-03\t09:35"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="2dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/imageView1"
        android:layout_below="@id/imageView1"
        android:background="@drawable/huibar2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/mysecurepaymentdepositresult_datatime"
        android:layout_alignTop="@id/imageView2"
        android:layout_marginTop="20dp"
        android:text="预计到账时间"
        android:textColor="@color/color_white"
        android:textSize="14sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/mysecurepaymentdepositresult_daozhang_datatime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/textView2"
        android:layout_below="@id/textView2"
        android:layout_marginTop="15dp"
        android:text="2015-06-05\t09:35"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/imageView2"
        android:layout_marginTop="10dp"
        android:background="@drawable/my_group_break_line" />

</RelativeLayout>