<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/myselectorprogresssure"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/my_break_line"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/background_up" >

        <Button
            android:id="@+id/myselectorprogresssure_backbtn"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="6dp"
            android:background="@drawable/btn_general_back"
            android:padding="10dp" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:text="客户确认"
            android:textColor="@color/color_white"
            android:textSize="20sp" />

        <Button
            android:id="@+id/myselectorprogresssure_telbtn"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="6dp"
            android:background="@drawable/phone" />
    </RelativeLayout>

    <TextView
        android:id="@+id/myselectorprogresssure_pricetxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@id/relativeLayout1"
        android:layout_marginLeft="20dp"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:text="批核金额"
        android:textColor="@color/color_white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/myselectorprogresssure_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/myselectorprogresssure_pricetxt"
        android:layout_alignBottom="@id/myselectorprogresssure_pricetxt"
        android:layout_centerHorizontal="true"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:text="10,000"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/myselectorprogresssure_pricetxt"
        android:background="@drawable/my_group_break_line" />

    <TextView
        android:id="@+id/myselectorprogresssure_qixiantxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@id/myselectorprogresssure_pricetxt"
        android:layout_below="@id/textView2"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:text="批核期限"
        android:textColor="@color/color_white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/myselectorprogresssure_qixian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/myselectorprogresssure_qixiantxt"
        android:layout_alignBottom="@id/myselectorprogresssure_qixiantxt"
        android:layout_alignLeft="@id/myselectorprogresssure_price"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:text="1个月"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/myselectorprogresssure_qixian"
        android:background="@drawable/my_group_break_line" />

    <TextView
        android:id="@+id/myselectorprogresssure_shenqingtxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@id/myselectorprogresssure_qixiantxt"
        android:layout_below="@id/textView3"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:text="申请业务"
        android:textColor="@color/color_white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/myselectorprogresssure_shenqing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/myselectorprogresssure_shenqingtxt"
        android:layout_alignBottom="@id/myselectorprogresssure_shenqingtxt"
        android:layout_alignLeft="@id/myselectorprogresssure_qixian"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:text="房贷代还"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/myselectorprogresssure_shenqing"
        android:background="@drawable/my_group_break_line" />

    <TextView
        android:id="@+id/myselectorprogresssure_jietxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@id/myselectorprogresssure_shenqingtxt"
        android:layout_below="@id/textView4"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:text="借款描述"
        android:textColor="@color/color_white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/myselectorprogresssure_jie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/myselectorprogresssure_jietxt"
        android:layout_alignBottom="@id/myselectorprogresssure_jietxt"
        android:layout_alignLeft="@id/myselectorprogresssure_shenqing"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:text="急需资金周转"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/myselectorprogresssure_jie"
        android:layout_alignBottom="@id/myselectorprogresssure_jie"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"
        android:text="元"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/myselectorprogresssure_jie"
        android:background="@drawable/my_group_break_line" />

    <TextView
        android:id="@+id/myselectorprogresssure_fuwufeitxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@id/myselectorprogresssure_jietxt"
        android:layout_below="@id/textView5"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:text="服务费"
        android:textColor="@color/color_white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/myselectorprogresssure_fuwufei"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/myselectorprogresssure_fuwufeitxt"
        android:layout_alignBottom="@id/myselectorprogresssure_fuwufeitxt"
        android:layout_alignLeft="@id/myselectorprogresssure_jie"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:text="2,000"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/myselectorprogresssure_fuwufei"
        android:layout_alignBottom="@id/myselectorprogresssure_fuwufei"
        android:layout_alignLeft="@id/textView6"
        android:text="元"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/myselectorprogresssure_fuwufei"
        android:background="@drawable/my_group_break_line" />

    <TextView
        android:id="@+id/myselectorprogresssure_paytxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@id/myselectorprogresssure_fuwufeitxt"
        android:layout_below="@id/textView7"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:text="到期还款"
        android:textColor="@color/color_white"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/myselectorprogresssure_pay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/myselectorprogresssure_paytxt"
        android:layout_alignBottom="@id/myselectorprogresssure_paytxt"
        android:layout_alignLeft="@id/myselectorprogresssure_fuwufei"
        android:paddingBottom="15dp"
        android:paddingTop="15dp"
        android:text="12,000"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/myselectorprogresssure_pay"
        android:layout_alignBottom="@id/myselectorprogresssure_pay"
        android:layout_alignLeft="@id/textView8"
        android:text="元"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/myselectorprogresssure_pay"
        android:background="@drawable/my_group_break_line"
        android:paddingBottom="15dp" />

    <CheckBox
        android:id="@+id/myselectorprogresssure_checkthird"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/myselectorprogresssure_paytxt"
        android:layout_below="@id/textView9"
        android:checked="true" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/myselectorprogresssure_checkthird"
        android:layout_alignBottom="@id/myselectorprogresssure_checkthird"
        android:layout_toRightOf="@id/myselectorprogresssure_checkthird"
        android:text="同意《三方借款协议》"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <CheckBox
        android:id="@+id/myselectorprogresssure_checkperson"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/myselectorprogresssure_checkthird"
        android:layout_below="@id/myselectorprogresssure_checkthird" 
        android:checked="true"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/myselectorprogresssure_checkperson"
        android:layout_alignBottom="@id/myselectorprogresssure_checkperson"
        android:layout_toRightOf="@id/myselectorprogresssure_checkperson"
        android:text="同意《个人代扣授权协议》"
        android:textColor="@color/color_white"
        android:textSize="12sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@drawable/my_break_line"
        android:orientation="horizontal"
        android:paddingBottom="10dp"
        android:paddingTop="6dp" >

        <Button
            android:id="@+id/myselectorprogresssure_surebtn"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="6dp"
            android:layout_weight="1"
            android:background="@drawable/start_round_sel"
            android:text="确定"
            android:textColor="@color/color_selector_break"
            android:textSize="14sp" />

        <Button
            android:id="@+id/myselectorprogresssure_giveupbtn"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_marginLeft="6dp"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:background="@drawable/start_round_sel"
            android:text="放弃"
            android:textColor="@color/color_selector_break"
            android:textSize="14sp" />
    </LinearLayout>

</RelativeLayout>